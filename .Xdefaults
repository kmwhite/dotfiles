! Xft Settings
Xft.antialias:				true
Xft.dpi:				96
Xft.hinting:				true
Xft.hintstyle:				1
Xft.rgba:				1
Xft.lcdfilter:				1

! XTerm Settings
!! Configuration
URxvt*loginShell:			true
URxvt*termName:				xterm-256color
URxvt*depth:				24
URxvt*saveLines:			65536
URxvt*visualBell:			true
URxvt*cutchars:				()*;<>[]{|}
URxvt*print-pipe:			cat > $(echo XTerm.dump.$(date +'%Y%M%d%H%m%S'))
!XTerm*keysym.Home:			\033[1~
!XTerm*keysym.End:			\033[4~
!XTerm*geometry:			80x24
!XTerm*deleteIsDEL:			true
!XTerm*backarrowKey:			true
!! Fonts
URxvt*font:				xft:Consolas:pixelsize=14:style=regular
URxvt*boldFont:				xft:Consolas:pixelsize=14:style=bold
!! Colors
URxvt*background:			rgb:00/00/00
URxvt*foreground:			rgb:a8/a8/a8
URxvt*color0:				rgb:00/00/00
URxvt*color1:				rgb:a8/00/00
URxvt*color2:				rgb:00/a8/00
URxvt*color3:				rgb:a8/54/00
URxvt*color4:				rgb:00/00/a8
URxvt*color5:				rgb:a8/00/a8
URxvt*color6:				rgb:00/a8/a8
URxvt*color7:				rgb:a8/a8/a8
URxvt*color8:				rgb:54/50/54
URxvt*color9:				rgb:f8/54/50
URxvt*color10:				rgb:50/fc/50
URxvt*color11:				rgb:f8/fc/50
URxvt*color12:				rgb:50/54/f8
URxvt*color13:				rgb:f8/54/f8
URxvt*color14:				rgb:50/fc/f8
URxvt*color15:				rgb:f8/fc/f8
URxvt*cursorColor:			rgb:ff/cc/00
!! Transparency
URxvt*transparent:			true
URxvt*tintColor:			gray
URxvt*shading:				25
!! Extensions
URxvt.perl-ext-common:			default,tabbed,matcher
!!! Tabbing
!!!! Shift+Down				new tab
!!!! Shift+Left				go to left tab
!!!! Shift+Right			go to right tab
!!!! Ctrl+Left				move tab to the left
!!!! Ctrl+Right				move tab to the right
!!!! Ctrl+D:				close tab
URxvt.tabbed.tabbar-fg:			2
URxvt.tabbed.tabbar-bg:			0
URxvt.tabbed.tab-fg:			3
URxvt.tabbed.tab-bg:			0
!!! Matching
URxvt.urlLauncher:			/usr/local/bin/chrome "%s"
URxvt.matcher.button:			1
URxvt.matcher.pattern.1:		\\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
!! Scrolling
URxvt*scrollBar:			false
URxvt*scrollstyle:			plain
URxvt*scrollBar_right:			false
URxvt*scrollColor:			rgb:77/77/77
URxvt*secondaryScroll:			true

! Dunst Configuration
Dunst*Font:				Consolas-10
Dunst*Geometry:				0x10-20+20
Dunst*IndicateHidden:			yes
Dunst*Format:				"%s %b"
Dunst*IdleThreshold:			120
Dunst*LowBackground:			#339933
Dunst*LowForeground:			#000000
Dunst*LowTimeout:			10
Dunst*NormalBackground:			#ffcc00
Dunst*NormalForeground:			#000000
Dunst*NormalTimeout:			10
Dunst*CriticalBackground:		#cc0000
Dunst*CriticalForeground:		#ffffff
Dunst*CriticalTimeout:			10

